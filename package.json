    "graphql-compose-connection": "^2.1.3",
    "graphql-compose-mongoose": "^1.4.8",
    "graphql-date": "^1.0.2",
    "graphql-server-express": "^0.5.1",
    "graphql-tag": "^1.2.2",
    "graphql-tools": "^0.9.0",
    "immutable": "~3.8.1",
    "is-url": "^1.2.2",
    "isomorphic-fetch": "^2.2.1",
