{
  "name": "Clogii",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "babel-node ./server/server.js",
    "dev-web": "next ./web",
    "import-data": "babel-node ./scripts/import-data-from-parse.js",
    "test": "jest",
    "lint": "eslint .",
    "compile:server": "babel server --out-dir build-server",
    "up-local": "npm run compile:server && . ./scripts/up-local",
    "down-local": ". ./scripts/down-local",
    "restart-local": "npm run down-local && npm run up-local"
