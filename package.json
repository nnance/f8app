{
  "name": "Clogii",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "babel-node ./server/server.js",
    "start:watch": "nodemon --watch server --exec babel-node ./server/server.js",
    "import-data": "babel-node ./scripts/import-data-from-parse.js",
    "test": "jest",
    "lint": "eslint js server",
    "compile:server": "babel server --out-dir build-server",
    "dev": "npm run dev:db && npm run start:watch",
    "dev:db": ". ./scripts/dev-db",
    "gen-seed": "babel-node ./scripts/seeds/gen-seed.js",
    "import-seed": "babel-node ./scripts/seeds/import.js",
    "remove-seed": "babel-node ./scripts/seeds/remove.js",
    "re-gen-seed": "npm run gen-seed && npm run remove-seed && npm run import-seed",
    "seed-episode-binary": "babel-node ./scripts/seeds/seed-episode-binary.js",
    "clean-db": "babel-node ./scripts/clean-db.js",
    "release": "standard-version"
  },
