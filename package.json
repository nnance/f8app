  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "~7.1.1",
    "babel-jest": "^18.0.0",
    "babel-polyfill": "^6.20.0",
    "babel-preset-es2015": "^6.18.0",
    "cz-conventional-changelog": "^2.0.0",
    "enzyme": "^2.7.0",
    "enzyme-to-json": "^1.4.5",
    "eslint": "^3.15.0",
    "eslint-config-airbnb": "^14.1.0",
