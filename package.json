    "dotenv": "^2.0.0",
    "express": "~4.14.0",
    "fbjs": "^0.8.8",
    "graphql": "^0.8.2",
    "graphql-tools": "^0.9.0",
    "immutable": "~3.8.1",
    "moment": "~2.17.1",
    "object-assign": "^4.1.0",
    "parse": "^1.9.2",
    "parse-dashboard": "~1.0.22",
    "parse-server": "~2.3.1",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-native": "^0.38.1",
    "react-native-code-push": "^1.16.1-beta",
    "react-native-fbsdk": "^0.4.0",
    "react-native-image-picker": "^0.24.1",
    "react-native-linear-gradient": "^1.6.2",
