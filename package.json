  "jest": {
    "haste": {
      "defaultPlatform": "ios",
      "platforms": [
        "ios",
        "android"
      ],
      "providesModuleNodeModules": [
        "react-native"
      ]
    },
    "preset": "react-native",
    "transformIgnorePatterns": [
      "node_modules/(?!react-native|react-clone-referenced-element|parse|graphql-comment|@exponent/react-native-read-more-text)"
    ],
    "setupFiles": [
      "dotenv/config",
      "./js/libs/test-setup"
    ]
  },
  "engines": {
    "node": ">=5.0",
    "npm": ">=3.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
